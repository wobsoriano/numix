{
  "name": "numix",
  "version": "0.0.3",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./composables": {
      "import": "./dist/runtime/composables/useLoaderData.mjs",
      "types": "./dist/runtime/composables/useLoaderData.d.ts"
    },
    "./form": {
      "import": "./dist/runtime/utils/form.mjs",
      "types": "./dist/runtime/utils/form.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "composables.d.ts"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "pnpm prepack && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "prepublishOnly": "pnpm prepack",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0",
    "@rollup/plugin-virtual": "^3.0.1",
    "@vue/compiler-sfc": "^3.2.45",
    "acorn-strip-function": "^1.0.0",
    "es-module-lexer": "^1.1.0",
    "esbuild": "^0.15.14",
    "h3": "^1.0.1",
    "ohash": "^1.0.0",
    "pathe": "^1.0.0",
    "radix3": "^1.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.0",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.27.0",
    "nuxt": "^3.0.0"
  }
}
