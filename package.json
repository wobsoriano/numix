{
  "name": "numix",
  "description": "Write your server code inside Vue SFC's and handle forms easily.",
  "version": "0.0.22",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./client": {
      "import": "./dist/runtime/client/index.mjs",
      "types": "./dist/runtime/client/index.d.ts"
    },
    "./server": {
      "import": "./dist/runtime/server/index.mjs",
      "types": "./dist/runtime/server/index.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "client.d.ts"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "pnpm prepack && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "prepublishOnly": "pnpm prepack",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0",
    "@rollup/plugin-virtual": "^3.0.1",
    "@vue/compiler-sfc": "^3.2.45",
    "es-module-lexer": "^1.1.0",
    "h3": "^1.0.1",
    "ofetch": "^1.0.0",
    "ohash": "^1.0.0",
    "pathe": "^1.0.0",
    "unplugin-strip-exports": "^0.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.0",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.27.0",
    "nuxt": "^3.0.0"
  }
}
