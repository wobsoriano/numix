{
  "name": "numix",
  "description": "Write your server code inside Nuxt pages and handle forms easily.",
  "version": "0.1.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./server": {
      "import": "./dist/runtime/server/index.mjs",
      "types": "./dist/runtime/server/index.d.ts"
    },
    "./types": {
      "import": "./dist/runtime/types/index.mjs",
      "types": "./dist/runtime/types/index.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "server.d.ts",
    "types.d.ts"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "prepublishOnly": "pnpm prepack",
    "release": "bumpp && npm publish",
    "lint": "eslint .",
    "update-deps": "taze -w && pnpm i",
    "postinstall": "cd playground && npx prisma migrate dev --name init && npx prisma db seed"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0",
    "@vuedx/compiler-sfc": "^0.7.2",
    "es-module-lexer": "^1.1.0",
    "escape-string-regexp": "^5.0.0",
    "fast-glob": "^3.2.12",
    "h3": "^1.0.2",
    "ofetch": "^1.0.0",
    "ohash": "^1.0.0",
    "pathe": "^1.0.0",
    "unplugin-strip-exports": "^0.1.8"
  },
  "devDependencies": {
    "@prisma/client": "^4.6.1",
    "taze": "^0.8.5",
    "esno": "^0.16.3",
    "prisma": "^4.6.1",
    "@antfu/eslint-config": "^0.31.0",
    "@nuxt/module-builder": "^0.2.1",
    "@nuxt/schema": "^3.0.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.27.0",
    "nuxt": "^3.0.0"
  }
}
