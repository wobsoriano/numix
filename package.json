{
  "name": "numix",
  "description": "Write your server code inside Nuxt pages and handle forms easily.",
  "version": "0.3.2",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./server": {
      "import": "./dist/runtime/server/index.mjs",
      "types": "./dist/runtime/server/index.d.ts"
    },
    "./types": {
      "import": "./dist/runtime/types/index.mjs",
      "types": "./dist/runtime/types/index.d.ts"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist",
    "server.d.ts",
    "types.d.ts"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "db:prepare": "cd playground && npx prisma migrate dev --name init && npx prisma db seed"
  },
  "dependencies": {
    "@nuxt/kit": "^3.12.4",
    "@vuedx/compiler-sfc": "^0.7.2",
    "es-module-lexer": "^1.2.1",
    "escape-string-regexp": "^5.0.0",
    "h3": "^1.6.6",
    "ofetch": "^1.0.1",
    "ohash": "^1.1.2",
    "pathe": "^1.1.0",
    "ufo": "^1.1.2",
    "unplugin-strip-exports": "^0.1.9"
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.3.9",
    "@nuxt/eslint-config": "^0.3.13",
    "@nuxt/module-builder": "^0.8.1",
    "@nuxt/schema": "^3.12.4",
    "@nuxt/test-utils": "^3.13.1",
    "@prisma/client": "^5.17.0",
    "@types/node": "^20.14.11",
    "changelogen": "^0.5.5",
    "eslint": "^9.7.0",
    "nuxt": "^3.12.4",
    "prisma": "^5.17.0",
    "typescript": "latest",
    "vitest": "^2.0.3",
    "vue-tsc": "^2.0.26"
  },
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a"
}
